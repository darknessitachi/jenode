<?xml version="1.0" encoding="UTF-8"?>
<framework>
	<application>
		<config name="Code">ZCMS</config>
		<config name="Name">ZCMS内容管理系统</config>
		<config name="DebugMode">true</config>
		<config name="Log.SQL">true</config>
		<config name="Log.PageServerCost">true</config>
		<config name="Log.PageClientCost">true</config>
 		<config name="LoginMethod">com.zving.platform.ui.LoginUI.submit</config>
		<config name="LoginPage">login.zhtml</config>
		<config name="MemberLoginPage">member/login</config>
		<config name="CodeSource">com.zving.platform.pub.PlatformCodeSource</config>
		<config name="ExistPlatformDB">true</config>
		<config name="DefaultLanguage">zh-cn</config>
		<config name="FrontDeploy">false</config>
	</application>
	<cron>
		<task id="com.zving.sitegroup.bl.InnerSyncTask" time="*/2 * * * *" disabled="true"></task>
		<task id="com.zving.media.impl.VideoScanTask" time="*/2 * * * *" disabled="true"></task>
	</cron>
</framework>
